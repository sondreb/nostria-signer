{
  "app": {
    "bundleId": "no.brainbox.nostria-signer",
    "icon": "icons/128x128@2x.png",
    "android": {
      "minSdkVersion": 24
    }
  },
  "android": {
    "signing": {
      "keystore": "${ANDROID_KEYSTORE_PATH}",
      "keystorePassword": "${ANDROID_KEYSTORE_PASSWORD}",
      "keyAlias": "${ANDROID_KEY_ALIAS}",
      "keyPassword": "${ANDROID_KEY_PASSWORD}"
    }
  },
  "build": {
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist/app/browser"
  },
  "bundle": {
    "identifier": "no.brainbox.nostria-signer"
  }
}
